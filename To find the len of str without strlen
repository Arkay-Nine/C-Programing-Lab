#include <stdio.h>

int main() {
    
    char str[30];
    int len = 0;
    
    printf("Enter your charaters =>");
    fgets(str,30,stdin);
    
    for (int i = 0; str[i] != '\0'; i++)

    {
        len++;

    }
    printf("Therefore, the lenght of given String is => %d",len-1);
    
    return 0;
}
